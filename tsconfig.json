{
	"compilerOptions": {
		"lib": [
			"dom",
			"es2015",
			"esnext.asynciterable"
		],
		"downlevelIteration": true,
		"module": "es6",
		"noImplicitAny": true,
		"outDir": "_build/",
		"paths": {
			"aframe" : ["../../@types/aframe"]
		},
		"removeComments": false,
		"sourceMap": true,
		"target": "es5",
		"moduleResolution": "node"
	}
}
